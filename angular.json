{
  "version": 1,
  "projects": {
    "i-love-iac": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/i-love-iac",
      "sourceRoot": "apps/i-love-iac/src",
      "prefix": "advanced-deployments",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/i-love-iac",
            "index": "apps/i-love-iac/src/index.html",
            "main": "apps/i-love-iac/src/main.ts",
            "polyfills": "apps/i-love-iac/src/polyfills.ts",
            "tsConfig": "apps/i-love-iac/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/i-love-iac/src/favicon.ico",
              "apps/i-love-iac/src/assets"
            ],
            "styles": ["apps/i-love-iac/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/i-love-iac/src/environments/environment.ts",
                  "with": "apps/i-love-iac/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "i-love-iac:build",
            "proxyConfig": "apps/i-love-iac/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "i-love-iac:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "i-love-iac:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/i-love-iac/tsconfig.app.json",
              "apps/i-love-iac/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/i-love-iac/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/i-love-iac/jest.config.js",
            "tsConfig": "apps/i-love-iac/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/i-love-iac/src/test-setup.ts"
          }
        },
        "deploy": {
          "builder": "@dev-thought/nx-deploy-it:deploy",
          "options": {
            "main": "apps/i-love-iac/infrastructure/index.ts",
            "provider": "gcp",
            "pulumi": {
              "gcp:project": "braze-migration-poc-286216",
              "customDomainName": "www.example.com",
              "useCdn": false
            }
          },
          "configurations": {
            "production": {
              "pulumi": {
                "useCdn": true
              }
            }
          }
        },
        "destroy": {
          "builder": "@dev-thought/nx-deploy-it:destroy",
          "options": {
            "main": "apps/i-love-iac/infrastructure/index.ts",
            "provider": "gcp"
          }
        }
      }
    },
    "i-love-iac-e2e": {
      "root": "apps/i-love-iac-e2e",
      "sourceRoot": "apps/i-love-iac-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/i-love-iac-e2e/cypress.json",
            "tsConfig": "apps/i-love-iac-e2e/tsconfig.e2e.json",
            "devServerTarget": "i-love-iac:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "i-love-iac:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/i-love-iac-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/i-love-iac-e2e/**/*"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/api-interfaces/tsconfig.lib.json",
              "libs/api-interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/api-interfaces/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "tsConfig": "libs/api-interfaces/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "test-app": {
      "root": "apps/test-app",
      "sourceRoot": "apps/test-app/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/test-app",
            "index": "apps/test-app/src/index.html",
            "main": "apps/test-app/src/main.tsx",
            "polyfills": "apps/test-app/src/polyfills.ts",
            "tsConfig": "apps/test-app/tsconfig.app.json",
            "assets": [
              "apps/test-app/src/favicon.ico",
              "apps/test-app/src/assets"
            ],
            "styles": ["apps/test-app/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/test-app/src/environments/environment.ts",
                  "with": "apps/test-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "test-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "test-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/test-app/tsconfig.app.json",
              "apps/test-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/test-app/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/test-app/jest.config.js",
            "tsConfig": "apps/test-app/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "test-app-e2e": {
      "root": "apps/test-app-e2e",
      "sourceRoot": "apps/test-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/test-app-e2e/cypress.json",
            "tsConfig": "apps/test-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "test-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/test-app-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/test-app-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "i-love-iac"
}
